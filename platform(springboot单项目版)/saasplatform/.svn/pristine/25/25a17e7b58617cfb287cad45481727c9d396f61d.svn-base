.headerTitle{height:100px;width:100%;background:rgba(0,0,0,.05);background:-moz-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));background:-o-linear-gradient(top, rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0));
 background:-webkit-gradient(linear, 0 0, 0 bottom, from(rgba(0, 0, 0, 0.4)), to(rgba(0, 0, 0, 0)));position:absolute;top:0;z-index:9999;}
.headerTitle .logo{height:50px;width:350px;margin-top:10px;margin-left:30px;background:url('/lsc-web/static/newImg/login/logo.png');background-size:100% 100%;float:left;}
.headerTitle .btnBox{float:right;}
.headerTitle .btnBox .login{display:inline-block;width:92px;height:34px;line-height:34px;font-size:14px;color:#fff;border:1px solid #fff;border-radius:2px;margin-top:20px;text-align:center;margin-right:30px;cursor:pointer;}
.appBox,.wechatBox{position:relative;}
.headerTitle .btnBox .app{display:inline-block;width:28px;height:28px;background:url('/lsc-web/static/newImg/login/phone.png');background-size:100% 100%;margin-right:20px;vertical-align:middle;}
.appPic{position:absolute;top:30px;right:-20px;display:none;}
.angle-wrapper {width: 140px;height: 140px;position: relative;margin: 20px auto;border: 1px solid #cccccc;background:#fff url('/lsc-web/static/newImg/login/app.jpg') no-repeat center;background-size:80% 80%;border-radius:4px;}
.angle-wrapper:before {  
    content: '';  
    width: 0;  
    height: 0;  
    border: 12px solid transparent;  
    border-bottom-color: #cccccc;  
    position: absolute;  
    right: 35%;  
    top: 0;  
    margin-top:-24px;
}  
.angle-wrapper:after {  
    content: "";  
    width: 0;  
    height: 0;  
    border: 10px solid transparent;  
    border-bottom-color: #FFFFFF;  
    position: absolute;  
    right: 36%;  
    top: 0;  
    margin-top:-20px;
} 
.appPic1{position:absolute;top:30px;right:20px;display:none;}
.angle-wrapper1 {width: 140px;height: 140px;position: relative;margin: 20px auto;border: 1px solid #cccccc;background:#fff url('/lsc-web/static/newImg/login/wechat.jpg') no-repeat center;background-size:80% 80%;border-radius:4px;}
.angle-wrapper1:before {  
    content: '';  
    width: 0;  
    height: 0;  
    border: 12px solid transparent;  
    border-bottom-color: #cccccc;  
    position: absolute;  
    right: 16%;  
    top: 0;  
    margin-top:-24px;
}  
.angle-wrapper1:after {  
    content: "";  
    width: 0;  
    height: 0;  
    border: 10px solid transparent;  
    border-bottom-color: #FFFFFF;  
    position: absolute;  
    right: 17.2%;  
    top: 0;  
    margin-top:-20px;
} 
.headerTitle .btnBox .wechat{display:inline-block;width:28px;height:28px;background:url('/lsc-web/static/newImg/login/wechat.png');background-size:100% 100%;margin-right:40px;vertical-align:middle;}

.sliders{position:relative;}
.picBox{width:100%;position:relative;min-height:400px;overflow:hidden;}
@media screen and (min-width: 1200px) and (max-width: 1400px) {
    .picBox{min-height:400px;}
}
@media screen and (max-width: 1400px) and (max-width: 1600px){
    .picBox{min-height:500px;}
}
@media  screen and (min-width: 1600px) and (max-width: 2000px) {
    .picBox{min-height:600px;}
}
.picBox .picItem{float:left;width:100%;position:absolute;left:100%;height:100%;}
.picItem img{width:100%;height:100%;}
.picBox .picItem.active{left:0;}
.navBox{position:absolute;bottom:30px;width:100%;text-align:center;}
.navBox .navDot{height:10px;width:10px;border-radius:50%;background:rgba(255,255,255,.4);display:inline-block;}
.navBox .navDot:nth-child(2){margin:0 10px;}
.navBox .navDot.active{background:#fff;}

.con1{padding:50px 0;}
.con1 .title{margin:0 auto;width:380px;height:50px;border:1px solid #808080;font-size:24px;text-align:center;line-height:50px;}
.title{width:520px;height:50px;border:1px solid #808080;line-height: 50px;margin: 0 auto;font-size:24px; text-align: center;}
.con1 .des{width:100%;text-align:center;margin-top:10px;}
.list{width:70%;margin:20px auto 0;}
.list .item{float:left;width:25%;text-align:center;}
.list .item .itemTitle{font-size:16px;font-weight:bold;margin-bottom:15px;}
.list .item .text{color:#808080;font-size:12px;line-height:20px;}
.list .item .imgBox{width:108px;height:108px;line-height:108px;border-radius:50%;display:inline-block;margin-bottom:20px;}
.list .item .imgBox.t1{background:url('/lsc-web/static/newImg/login/t1.png') no-repeat center;;background-size:80% 80%;}
.list .item .imgBox.t1:hover{background:#fc4c82 url('/lsc-web/static/newImg/login/t1b.png') no-repeat center;background-size:80% 80%;}
.list .item .imgBox.t2{background:url('/lsc-web/static/newImg/login/t2.png') no-repeat center;;background-size:80% 80%;}
.list .item .imgBox.t2:hover{background:#fc4c82 url('/lsc-web/static/newImg/login/t2b.png') no-repeat center;background-size:80% 80%;}
.list .item .imgBox.t3{background:url('/lsc-web/static/newImg/login/t3.png') no-repeat center;;background-size:80% 80%;}
.list .item .imgBox.t3:hover{background:#fc4c82 url('/lsc-web/static/newImg/login/t3b.png') no-repeat center;background-size:80% 80%;}
.list .item .imgBox.t4{background:url('/lsc-web/static/newImg/login/t4.png') no-repeat center;;background-size:80% 80%;}
.list .item .imgBox.t4:hover{background:#fc4c82 url('/lsc-web/static/newImg/login/t4b.png') no-repeat center;background-size:80% 80%;}

.con2{background:url('/lsc-web/static/newImg/login/conBg.jpg');background-size:100% 100%;width:100%;height:420px;padding-top:30px;}
.con2 .winBg{width:1002px;height:360px;margin:0 auto;background-size:100% 100%;}

.footer{background:#122137;height:40px;text-align:center;color:#babcc0;line-height:40px;font-size:12px;}

